<?xml version="1.0" encoding="utf-8" ?>
<component name="URLTask" extends="Task">
  <interface>
    <field id="url" type="string" />
    <field id="timeout" type="integer" value="10000" />

    <field id="success" type="boolean" />
    <field id="responseCode" type="integer" />
    <field id="content" type="string" alwaysNotify="true" />
  </interface>
  <script type="text/brightscript" uri="URLTask.brs" />
</component>
